#!/bin/bash

echo "Start deploy"
ssh -tq consultation@pprod.consultation.etalab.gouv.fr "bash -lc 'cd /home/consultation/consultation.gouv.fr && git pull'"
ssh -tq consultation@pprod.consultation.etalab.gouv.fr "bash -lc 'cd /home/consultation/consultation.gouv.fr && bundle exec jekyll doctor'"
ssh -tq consultation@pprod.consultation.etalab.gouv.fr "bash -lc 'cd /home/consultation/consultation.gouv.fr && bundle exec jekyll build'"
echo "Deployed Successfully!"

exit 0
